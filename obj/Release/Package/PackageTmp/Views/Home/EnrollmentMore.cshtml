    <!--enrolment modal-->
    <!--Document modal-->
    <div class="modal fade doc1" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-xl modal-dialog-centered">
            <div class="modal-content " id="docPrint1">
                <div class="modal-header">
                    <h2 class="green1">Enrolment Doc</h2>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">x</span>
                    </button>
                </div>
