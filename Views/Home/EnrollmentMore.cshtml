                    <li><a><span></span></a></li>
                    <li><a>now<br />1/1</a></li>
                    <li><a href="#"><span class="icon-home">@Html.ActionLink(" ", "Index", "Home", new { @class = "icon-home" })</span></a></li>
                    <li><a>next<br />1/1</a></li>
                    <li><a href="#"></a></li>

                </ul>
            </div>
            <div id="cn-overlay" class="cn-overlay"></div>
            <!-- End Nav Structure -->

        </div><!-- /container -->

    </section>
